{
    "[mdx]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 80
    },
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.formatOnPaste": true,
    "workbench.startupEditor": "newUntitledFile",
    "editor.fontSize": 13,
    "terminal.integrated.fontSize": 13,
    "editor.minimap.enabled": false,
    "window.zoomLevel": -1,
    "editor.tabSize": 2, 
    "editor.rulers": [80],
    "editor.tokenColorCustomizations": {
        "strings": "#458b00",
        "variables": "#ee2f01",
        "comments": "#737373",
        "keywords": "#a81143",
        "functions": "#007bff",
        // "numbers": "#FF0000",
        // "types": "#FF0000",
        "textMateRules": [
            {
                "scope": [
                    "meta.method-call",
                    "meta.tag.js",
                    "meta.tag.js punctuation.section.embedded",
                    "meta.brace.round.js",
                    "meta.method",
                    "punctuation.terminator.statement.js",
                    "source.js punctuation.definition.block",
                    "variable.other.readwrite.js",
                    "punctuation.definition.parameters",
                    "meta.class.js",
                    "source.js punctuation.separator.comma",
                    "meta.brace.square.js",
                    "meta.definition.property.js variable",
                    "meta.class.php",
                    "meta.class.body",
                    "keyword.operator",
                ],
                "settings": {
                    "foreground": "#33302e",
                }
            },
            {
                "scope": [
                    "entity.other.inherited-class",
                    "string",
                ],
                "settings": {
                    "foreground": "#458b00",
                }
            },
            {
                "scope": [
                    "storage",
                    "keyword",
                ],
                "settings": {
                    "foreground": "#a81143",
                }
            },
            {
                "scope": [
                    "variable",
                    "variable.other.readwrite.alias.js",
                    "meta.export.default.js variable.other.readwrite.js",
                ],
                "settings": {
                    "foreground": "#ee2f01",
                }
            },
            {
                "scope": [
                    "storage.type.property.js",
                ],
                "settings": {
                    "foreground": "#2c6d9c",
                }
            },
        ],
    },
    "workbench.colorTheme": "Atom One Light",
    "workbench.colorCustomizations": {
        "editorRuler.foreground": "#cad7d9",
        "editorIndentGuide.background": "#cad7d9",

        "editor.background": "#fff1e5",
        "editor.foreground": "#000",
        "editor.lineHighlightBackground": "#fff1e5",
        "editor.lineHighlightBorder": "#fff9f4",
        "editorLineNumber.activeForeground": "#000",
        
        "editor.selectionBackground": "#cad7d9",
        "editor.wordHighlightBackground": "#e9e4db",

        "sideBar.background": "#f2e5da",
        "sideBar.foreground": "#333",
        "sideBar.border": "#e9e4db",
        "sideBarSectionHeader.background": "#f2dfce",
        "sideBarSectionHeader.foreground": "#000",

        "list.activeSelectionBackground": "#cce6ff",
        "list.inactiveSelectionBackground": "#fff9f4",
        "list.hoverBackground": "#cad7d9",

        "editorGroup.border": "#0d7680",
        "editorGroupHeader.tabsBackground": "#f2dfce",
        
        "tab.border": "#9c9e9e",
        "tab.unfocusedActiveBackground": "#cad7d9",
        "tab.unfocusedActiveForeground": "#ffffff",
        "tab.activeBackground": "#0d7680",
        "tab.activeForeground": "#ffffff",
        "tab.inactiveBackground": "#f2e5da",
        "tab.inactiveForeground": "#0d7680",

        "activityBar.background": "#f2dfce",
        "activityBar.foreground": "#000000",
        "activityBarBadge.background": "#0d7680",

